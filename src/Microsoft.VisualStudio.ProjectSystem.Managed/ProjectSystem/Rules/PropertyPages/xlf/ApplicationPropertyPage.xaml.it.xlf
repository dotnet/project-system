        <source>Critical only</source>
        <target state="translated">Solo errori critici</target>
        <note />
      </trans-unit>
      <trans-unit id="EnumValue|NuGetAuditLevel.high|DisplayName">
        <source>High and above</source>
        <target state="translated">Elevato e superiore</target>
