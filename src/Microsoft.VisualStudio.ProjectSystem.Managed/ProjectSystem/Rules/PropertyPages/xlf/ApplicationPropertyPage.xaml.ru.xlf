        <source>Icon and manifest</source>
        <target state="translated">Значок и манифест</target>
        <note />
      </trans-unit>
      <trans-unit id="EnumValue|ResourceSpecificationKind.ResourceFile|DisplayName">
        <source>Resource file</source>
