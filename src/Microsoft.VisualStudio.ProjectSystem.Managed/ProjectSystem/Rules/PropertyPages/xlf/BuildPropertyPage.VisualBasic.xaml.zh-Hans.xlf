        <note>{Locked}</note>
      </trans-unit>
      <trans-unit id="EnumValue|OptionInfer.On|DisplayName">
        <source>On</source>
        <target state="new">On</target>
        <note>{Locked}</note>
      </trans-unit>
      <trans-unit id="EnumValue|OptionStrict.Off|DisplayName">
        <source>Off</source>
