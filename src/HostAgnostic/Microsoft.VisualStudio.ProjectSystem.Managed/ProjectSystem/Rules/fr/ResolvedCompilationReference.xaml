<Rule Name="ResolvedCompilationReference" DisplayName="Разрешенная ссылка на компиляцию" PageTemplate="generic" Description="Свойства разрешенной ссылки на компиляцию" xmlns="http://schemas.microsoft.com/build/2009/properties">
  <Rule.DataSource>
    <DataSource Persistence="ResolvedReference" ItemType="ReferencePathWithRefAssemblies" HasConfigurationCondition="False" SourceOfDefaultValue="AfterContext" SourceType="TargetResults" MSBuildTarget="CollectResolvedCompilationReferencesDesignTime" />
  </Rule.DataSource>
  <StringProperty Name="ResolvedPath" ReadOnly="True" DisplayName="Путь" Description="Расположение сборки, на которую указывает ссылка.">
    <StringProperty.DataSource>
      <DataSource PersistedName="Identity" SourceOfDefaultValue="AfterContext" />
    </StringProperty.DataSource>
  </StringProperty>
  <StringProperty Name="OriginalPath" ReadOnly="True" Visible="False" />
  <StringProperty Name="CopyUpToDateMarker" ReadOnly="True" Visible="False" />
</Rule>